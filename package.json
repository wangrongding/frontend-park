{
  "name": "vue-boot",
  "private": true,
  "description": "这是一个有趣的前端趣味知识公园~该项目是我平时捣鼓前端相关技术的一些案例集合。",
  "license": "MIT",
  "keywords": [
    "JavaScript",
    "Vue",
    "Canvas",
    "Three.js",
    "Fabric.js"
  ],
  "author": {
    "name": "wangrongding",
    "email": "wangrongding@qq.com",
    "url": "https://www.fedtop.com/"
  },
  "homepage": "https://wangrongding.github.io/frontend-park/",
  "repository": {
    "type": "git",
    "url": "https://github.com/wangrongding/frontend-park.git"
  },
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "format": "prettier --write src",
    "lint": "eslint \"src/**/*{.vue,.js,.ts,.jsx,.tsx}\" --fix",
    "lintAll": "eslint . --ext .vue,.js,.ts,.jsx,.tsx src/ --fix",
    "stylelint": "stylelint \"src/**/*.*\" --fix",
    "stylelint-check": "stylelint-config-prettier-check",
    "fal": "npm run format && npm run stylelint && npm run lint",
    "prepare": "husky install",
    "preinstall": "npx npm-limit yarn",
    "cz": "cz"
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx,css,sass,scss,md}": [
      "prettier --write"
    ],
    "*.{vue,css,sass,scss}": [
      "stylelint --fix"
    ],
    "*.{vue,js,jsx,ts,jsx,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.6",
    "@multiavatar/multiavatar": "^1.0.7",
    "@types/node": "^17.0.45",
    "axios": "^0.27.2",
    "dat.gui": "^0.7.9",
    "element-plus": "^2.2.6",
    "fabric": "^5.2.1",
    "gcoord": "^0.3.2",
    "pinia": "^2.0.13",
    "stats.js": "^0.17.0",
    "three": "^0.141.0",
    "tone": "^14.7.77",
    "vue": "^3.2.37",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@types/fabric": "^4.5.7",
    "@types/multiavatar__multiavatar": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "commitizen": "^4.2.4",
    "commitlint-config-cz": "^0.13.3",
    "cz-conventional-changelog": "3.3.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "postcss-html": "^1.3.0",
    "prettier": "^2.6.2",
    "sass": "^1.50.0",
    "stylelint": "^14.6.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.7.1",
    "unplugin-vue-components": "^0.19.5",
    "vite": "^2.9.1",
    "vue-tsc": "^0.33.9"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
